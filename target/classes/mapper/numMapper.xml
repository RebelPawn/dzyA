<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hp.controller.NumController">
    <resultMap id="numMap" type="com.hp.bean.Nummodel">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="numId" column="numId" jdbcType="BIGINT"/>
        <result property="data" column="SQ" jdbcType="DECIMAL"/>
    </resultMap>
    <select id="selectSQ" resultMap="numMap" >
        select id,numId,SQ from num
    </select>



</mapper>